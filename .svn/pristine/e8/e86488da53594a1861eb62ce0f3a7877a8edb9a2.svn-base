  line-height: 30px;
}

.scoopDetail .captionMation div.div2 a img {
  position: absolute;
  height: 30px;
  width: 30px;
  right: 0;
  top: 0;
}

.scoopDetail .captionMation div.div2 a span.tel {
  color: #1766b3;
}

.DetailIntroduce {
  border-bottom: 10px solid #ededed;
  background: white;
  overflow: hidden;
}

.DetailIntroduce .introduceItem > div {
  margin: 0 10px;
}

.DetailIntroduce .introduceItem > p {
  color: #666666;
  margin: 10px 10px 0;
  line-height: 25px;
}

.DetailIntroduce .introduceItem div.item {
  margin-bottom: 10px;
}

