}

.regeditCost p {
  height: 50px;
  line-height: 50px;
  font-size: 16px;
  margin-left: 10px;
}

.regeditCost p span {
  color: #d41116;
  font-weight: bold;
  font-size: 16px;
}

.regeditPay .regeditOrder {
  height: 40px;
  line-height: 40px;
  overflow: hidden;
}

.regeditPay .regeditOrder p.regeditOrderL {
  float: left;
  margin-left: 10px;
  color: #666666;
}

.regeditPay .regeditOrder p.regeditOrderR {
  float: right;
  margin-right: 10px;
  color: #d41116;
}

.regeditPay p.orderCost {
  height: 50px;
  background: white;
  line-height: 50px;
  margin-bottom: 10px;
}

.regeditPay p.orderCost span:first-child {
  margin-left: 10px;
}

.regeditPay .orderPrice {
  height: 150px;
  text-align: center;
  line-height: 150px;
  font-size: 35px;
  font-weight: bold;
}

.regeditPay .orderPrice span {
  font-size: 35px;
  font-weight: bold;
}

.regeditPay .wxPay, .payingMember .wxPay {
  width: 94%;
  height: 50px;
  color: white;
  font-size: 18px;
  background: #d41116;
