*{
  font-family: '微软雅黑';
}
.scoopList {
  overflow: hidden;
  border-bottom: 20px solid #e6e6e6;
}

.scoopList .listCover {
  position: relative;

}

.scoopList .listMes {
  margin-bottom: 2px;
  box-shadow: 0px 1px 2px 0px rgba(101, 101, 101, 0.45);
}

.scoopList .listCover img {
  width: 100%;
}

.scoopList .listCover div {
  position: absolute;
  left: 20px;
  top: 20px;
  z-index: 5;
}

.scoopList .listCover div p {
  color: white;
  font-weight: bold;

}

.scoopList .listCover div p span {
  font-size: 14px;
}

.scoopList .listCover div p:first-child {
  font-size: 24px;
  margin-bottom: 5px;
}

.scoopList .listCover div p:first-child span {
  font-size: 24px;
}

.scoopList .listMes > div {
  overflow: hidden;
  margin: 0 2%;
  padding: 5px 0;
}

.scoopList .listMes .listMesB {
  position: relative;
  padding-top: 10px;
}

.scoopList .listMesB button {
  position: absolute;
  right: 0;
  top: 8px;
  background: #d41116;
  color: white;
  width: 70px;
  height: 25px;
  text-align: center;
  border-radius: 3px;
}

.scoopList .listMesB p {
  font-size: 16px;
}

.scoopList .listMesT p {
  float: left;
  width: 65%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 14px;
  color: #8f8f8f;
}

.scoopList .listMesT p.Tright {
  float: right;
  width: 100px;
  text-align: right;
}

.scoopDetail .DetailCaption {
  border-bottom: 10px solid #ededed;
  padding-bottom: 10px;
  background: white;
}

.scoopDetail .captionImg {
  height: 210px;
}

.scoopDetail .captionImg img {
  height: 100%;
  width: 100%;
}

.scoopDetail .captionMation {
  margin: 0 10px;
}

.scoopDetail .captionMation p {
  margin-top: 10px;
}

.scoopDetail .captionMation p.p1 {
  font-size: 16px;
}

.scoopDetail .captionMation p.p2 {
  font-size: 14px;
  color: #666666;
}

.scoopDetail .captionMation div.div1 {
  height: 25px;
