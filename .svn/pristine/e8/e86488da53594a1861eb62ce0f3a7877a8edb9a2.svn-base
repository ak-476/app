/*个人信息*/
.personal_detail {
  width: 100%;
  height: 100%;
}

.personal_detail .detail_img {
  overflow: hidden;
  text-align: center;
}

.personal_detail .detail_img img {
  margin: 31px auto 15px;
  width: 75px;
  border-radius: 37.5px;
}

.personal_detail .detail_img p {
  text-align: center;
}

.personal_detail .detail_mation {
  margin-top: 50px;
}

.personal_detail .detail_mation ul {
  overflow: hidden;
  border-bottom: 1px solid #ccc;
  margin: 0 10px;
  background: url(../../assets/images/gradeHa.png) right center no-repeat;
  background-size: 8px 13px;
  position: relative;
}

.personal_detail .detail_mation ul li {
  height: 50px;
  line-height: 50px;
  float: left;
  color: #8f8f8f;
}

.personal_detail .detail_mation ul select {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  text-align: right;
  opacity: 0;
}

.personal_detail .detail_mation ul li.detail_title {
  float: right;
  margin-right: 15px;
  color: #333;
  width: 66%;
  text-align: right;
}
.personal_detail .detail_mation ul li.detail_title a span{
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  width: 100%;
}
.personal_detail .detail_btn {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 70px;
  width: 100%;
  overflow: hidden;
  background: white;
}

.personal_detail .detail_btn button {
  display: block;
  width: 92%;
  margin-left: 4%;
  height: 50px;
  margin-top: 10px;
  background: #d41116;
  color: white;
  font-size: 16px;
  text-align: center;
  line-height: 50px;
  border-radius: 5px;
}

.monolayer {
  display: none;
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

.monolayer .domOpcity {
  position: absolute;
  left: 0;
  top: 0;
  height: 100%;
  width: 100%;
  background: black;
  opacity: 0.7;
}

.monolayer .domSuspend {
  width: 300px;
  position: absolute;
  left: 50%;
  margin-left: -150px;
  top: 150px;
  background: white;
  border-radius: 5px;
  overflow: hidden;
}

.monolayer .domSuspend p {
  text-align: center;
  font-size: 16px;
  height: 50px;
  line-height: 50px;
}

.monolayer .domSuspend .textInput {
  margin: 0 20px 20px;
  padding: 10px 0;
  background: white;
  border: 1px solid #ccc;
  border-radius: 5px;
}

.monolayer .domSuspend .textInput input {
  font-size: 16px;
  width: 100%;
  text-indent: 10px;
}

.monolayer .domSuspend .phoneInput {
  margin: 0 20px 20px;
  overflow: hidden;

}

.monolayer .domSuspend .phoneInput input {
  font-size: 16px;
  width: 55%;
  text-indent: 10px;
  float: left;
  height: 48px;
  border-radius: 5px;
  border: 1px solid #ccc;
}

.monolayer .domSuspend .phoneInput button {
  font-size: 16px;
  width: 40%;
  text-indent: 0;
  float: left;
  text-align: center;
  height: 50px;
  text-align: center;
  color: white;
  background: #1766b3;
  border-radius: 5px;
  float: right;
}

.monolayer .domSuspend .phoneInput button[disabled] {
  background: #666;

}

.monolayer .domSuspend .btnClick {
  border-top: 1px solid #ccc;
  margin-top: 20px;
  overflow: hidden;
  background-color: #ccc;
}

.monolayer .domSuspend .btnClick button {
  height: 50px;
  line-height: 50px;
  text-align: center;
  width: 49.5%;
  float: left;
  font-size: 16px;
  background-color: white
}

.monolayer .domSuspend .btnClick button.cancel {
  margin-right: 0.5%;
}

.monolayer .domSuspend .btnClick button.ensure {
  color: #1766b3;
}

.growLife {
