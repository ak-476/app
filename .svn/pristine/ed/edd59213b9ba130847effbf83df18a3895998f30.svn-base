a{outline:none;text-decoration:none;hide-focus:expression(this.hideFocus=true);}
a:focus{outline:none;}
address,caption,cite,code,dfn,em,th,var{font-style:normal;font-weight:400;}
caption,th{text-align:left;}
q:before,q:after{content:'';}
h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:400;margin:0;padding:0;}
input,button,textarea,select{font-family:inherit;margin:0;padding:0;font-size:16px;outline:0 none;}
textarea{resize:none;overflow:auto;}
html{overflow-y:scroll;}
body{background-color:#fff;color:#666;}
iframe{position:relative;vertical-align:top;}

svg{overflow:hidden;}
clear{clear:both;}
.clearfix:after{content:'\20';display:block;clear:both;visibility:hidden;line-height:0;height:0;}
.clearfix{display:block;zoom:1;}
html[xmlns] .clearfix{display:block;}
* html .clearfix{height:1%;}

/*FF HACK*/@-moz-document url-prefix(){input.submit,input.reset{padding-bottom:3px;}}
/*CHROME HACK*/@media screen and (-webkit-min-device-pixel-ratio:0){}
input{vertical-align:middle;}
input[type=submit],input[type=button]{-webkit-appearance:none;}
input[type=text],input[type=submit],input[type=button],input[type=number],select{
	box-sizing:border-box;-webkit-box-sizing:border-box;width:100%;}
img{vertical-align:top;border:0;border:none;color:transparent;font-size:0;-ms-interpolation-mode:bicubic;}
label{vertical-align:middle;display:inline-block;}